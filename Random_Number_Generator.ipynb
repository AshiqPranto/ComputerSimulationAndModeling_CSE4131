    "seed = 675248\n",
    "randoms = []\n",
    "val = seed\n",
    "prev = val\n",
    "while val:\n",
    "    randoms.append(val)\n",
    "    prev = val\n",
    "    x = val ** 2\n",
    "    temp = f'{x:012d}'\n",
    "    val = int(temp[3 : -3])\n",
    "    if val == prev:\n",
    "        break\n",
    "print(randoms)\n",
    "print(len(randoms))\n"
